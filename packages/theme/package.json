{
  "name": "@sun-ui/theme",
  "version": "1.0.0",
  "description": "Pre-configured MUI themes with beautiful defaults. Light and dark themes included.",
  "homepage": "https://sun-ui.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tahmidbintaslim/sun-ui.git",
    "directory": "packages/theme"
  },
  "keywords": [
    "mui",
    "theme",
    "dark-mode",
    "light-theme",
    "design-system"
  ],
  "author": "Tahmid Bin Taslim <hello@tahmir.dev>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@mui/material": "^7.3.5",
    "@storybook/react": "^10.1.4",
    "@sun-ui/tokens": "workspace:*",
    "storybook": "^10.1.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-links": "^10.1.4",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.24.2",
    "@types/react": "^19.2.7",
    "eslint-plugin-storybook": "^10.1.4",
    "typescript": "^5.9.3"
  }
}
