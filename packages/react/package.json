        "lint": "eslint src",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@mui/material": "^7.3.5",
        "@sun-ui/theme": "workspace:*",
        "@sun-ui/tokens": "workspace:*"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-links": "^8.6.14",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^16.3.0",
        "@types/react": "^19.2.7",
        "vitest": "^4.0.0"
    }
}