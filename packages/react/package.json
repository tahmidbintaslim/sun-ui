{
  "name": "@sun-ui/react",
  "version": "1.0.0",
  "description": "Beautiful React components built on MUI. 7 design variants, zero config, 100% TypeScript.",
  "homepage": "https://sun-ui.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tahmidbintaslim/sun-ui.git",
    "directory": "packages/react"
  },
  "keywords": [
    "react",
    "components",
    "ui-library",
    "design-system",
    "material-ui",
    "mui",
    "button",
    "form",
    "accessible"
  ],
  "author": "Tahmid Bin Taslim <hello@tahmir.dev>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@mui/material": "^7.3.5",
    "@storybook/react": "^10.1.4",
    "@sun-ui/theme": "workspace:*",
    "@sun-ui/tokens": "workspace:*",
    "storybook": "^10.1.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-links": "^10.1.4",
    "@storybook/react-vite": "^10.1.2",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "eslint-plugin-storybook": "^10.1.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
