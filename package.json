{
  "name": "sun-ui",
  "version": "1.0.0",
  "description": "MUI components, reimagined. 7 design variants. Zero config. 100% TypeScript.",
  "homepage": "https://sun-ui.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tahmidbintaslim/sun-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "material-ui",
    "mui",
    "typescript",
    "accessible",
    "dark-mode"
  ],
  "author": "Tahmid Bin Taslim <hello@tahmir.dev>",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    },
    "onlyBuiltDependencies": [
      " ",
      "/",
      "@",
      "@swc/core",
      "a",
      "b",
      "c",
      "d",
      "e",
      "esbuild",
      "g",
      "i",
      "l",
      "m",
      "n",
      "o",
      "p",
      "r",
      "s",
      "t",
      "u",
      "unrs-resolver",
      "w",
      "x"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:storybook": "vitest --project=storybook --config apps/docs/vitest.config.ts",
    "test:storybook:ci": "vitest run --project=storybook --config apps/docs/vitest.config.ts",
    "test:ci": "vitest run",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\" --ignore-path .gitignore",
    "check:all": "pnpm lint && pnpm type-check && pnpm test:ci",
    "dev": "turbo run dev --parallel",
    "storybook:build": "pnpm --filter=storybook storybook:build",
    "storybook:dev": "pnpm --filter=storybook storybook",
    "clean": "turbo run clean && rm -rf node_modules pnpm-lock.yaml"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-vitest": "^10.1.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/browser": "^4.0.14",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.39.1",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.3",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@sun-ui/core": "workspace:*",
    "@sun-ui/icons": "workspace:*",
    "@sun-ui/react": "workspace:*",
    "@sun-ui/theme": "workspace:*",
    "@sun-ui/tokens": "workspace:*",
    "pnpm": "^10.24.0",
    "storybook": "^10.1.4"
  }
}
